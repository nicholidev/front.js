{
  "private": true,
  "name": "tutorial",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "cp -r ../../build/node_modules/* node_modules",
    "build": "node node_modules/@remix-run/cli build",
    "predev": "cp -r ../../build/node_modules/* node_modules",
    "dev": "concurrently \"node node_modules/@remix-run/cli run\" \"cross-env NODE_ENV=development nodemon -w app -e md,mdx,js,jsx,ts,tsx server.js\"",
    "prestart": "cp -r ../../build/node_modules/* node_modules",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.5"
  }
}
