{
  "private": true,
  "name": "tutorial",
  "version": "1.0.0",
  "scripts": {
    "install_remix": "mkdir -p node_modules && cp -r ../../build/node_modules/* node_modules",
    "prebuild": "yarn run install_remix",
    "build": "node node_modules/@remix-run/dev/cli.js build2",
    "preremixrun": "yarn run install_remix",
    "remixrun": "node node_modules/@remix-run/dev/cli.js run3",
    "prestart": "yarn run install_remix",
    "start": "node node_modules/@remix-run/serve/cli.js ./build"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "morgan": "^1.10.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "pm2": "^4.5.1"
  },
  "sideEffects": false
}
