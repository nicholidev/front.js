{
  "name": "vercel-fixture",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "cp -r ../../build/node_modules/* node_modules",
    "build": "node node_modules/@remix-run/cli build",
    "predev": "cp -r ../../build/node_modules/* node_modules",
    "dev": "concurrently \"node node_modules/@remix-run/cli run\" \"vercel dev\""
  },
  "dependencies": {
    "@vercel/node": "^1.8.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "engines": {
    "node": "12"
  }
}
